{
  "name": "jsql-vue-app",
  "version": "0.1.0",
  "scripts": {
    "serve": "concurrently --kill-others \"grunt dev\" \"vue-cli-service serve\" \"npm run jsql-watch\"",
    "jsql-watch": "jsql-cli --debug --apiKey=dawid.senko@jsql.it --input=/src/**/*.* --extensions=js,vue --devKeyFileName=test-key.key --env=local --watch",
    "jsql-build": "jsql-cli --debug --apiKey=dawid.senko@jsql.it --output=/dist/js/app.edaa8721.js --input=/dist/js/app.edaa8721.js --devKeyFileName=test-key.key --env=local",
    "build": "vue-cli-service build && npm run jsql-build",
    "vue-serve": "vue-cli-service serve"
  },
  "dependencies": {
    "jsql-axios": "latest",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "concurrently": "^4.1.0",
    "grunt": "^1.0.4",
    "grunt-concurrent": "^2.3.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-preprocess": "^5.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
